{
    "name": "magicoli/product-addons-autofill",
    "description": "WooCommerce Product Add-ons Autofill",
    "type": "wordpress-plugin",
    "license": "AGPL-V3",
    "autoload": {
        "psr-4": {
            "Magicoli\\ProductAddonsAutofill\\": "src/"
        }
    },
    "extra": {
      "installer-types": ["library"],
      "installer-paths": {
        "lib/meta-box/{$name}": ["vendor:wpmetabox","vendor:meta-box"],
        "lib/{$vendor}/{$name}": ["type:wordpress-plugin"]
      }
    },
    "authors": [
        {
            "name": "Olivier van Helden",
            "email": "olivier@van-helden.net"
        }
    ],
    "require": {
        "wpmetabox/meta-box": "^5.6",
        "meta-box/mb-settings-page": "dev-master"
    },
    "config": {
        "allow-plugins": {
            "composer/installers": true
        }
    }
}
